<?xml version="1.0" encoding="UTF-8"?>

<Shell
    x:Class="TestSpanGestures.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TestSpanGestures"
    Shell.FlyoutBehavior="Disabled"
    Title="TestSpanGestures">

    <TabBar Route="Main">
        <Tab Title="SpanInView">
            <ShellContent ContentTemplate="{DataTemplate local:SpanInView}" Route="SpanInView" />
        </Tab>
        <Tab Title="SpanInCollection">
            <ShellContent ContentTemplate="{DataTemplate local:SpanInCollectionView}" Route="SpanInCollection" />
        </Tab>
    </TabBar>

</Shell>